{
    "name": "carlin/laravel-data-swagger",
    "type": "library",
    "keywords": [
        "laravel",
        "swagger",
        "api",
        "OpenApi",
        "documentation",
        "API"
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": ">=9",
        "darkaonline/l5-swagger": "^8.6",
        "barryvdh/reflection-docblock": "^2.1",
        "phpdocumentor/type-resolver": "^1.8 || ^1.7",
        "carlin/laravel-dict": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Carlin\\LaravelDataSwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Carlin\\LaravelDataSwagger\\LaravelDataSwaggerServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "carlin",
            "email": "rjwangnixingfu@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "orchestra/testbench": "^8.22"
    }
}
